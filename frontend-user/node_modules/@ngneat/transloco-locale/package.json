    "type": "git",
    "url": "https://github.com/ngneat/transloco"
  },
  "keywords": [
    "angular",
    "angular 2",
    "i18n",
    "localization",
    "l10n",
    "angular translate",
    "angular i18n",
    "transloco"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": ">=13.0.0",
    "@ngneat/transloco": ">=4.0.0",
