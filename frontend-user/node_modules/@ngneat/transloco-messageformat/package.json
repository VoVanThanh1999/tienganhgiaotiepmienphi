{
  "name": "@ngneat/transloco-messageformat",
  "version": "4.1.0",
  "description": "Transpiler for Transloco that uses messageformat.js to compile translations using ICU syntax for handling pluralization and gender",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/ngneat/transloco/issues"
  },
  "homepage": "https://ngneat.github.io/transloco/docs/plugins/message-format",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngneat/transloco"
  },
  "keywords": [
    "angular",
    "angular 2",
    "declension",
    "i18n",
    "icu",
    "gender",
    "messageformat",
    "transloco",
    "pluralization",
    "translate"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": ">=13.0.0",
    "@ngneat/transloco": ">=4.2.0",
    "rxjs": ">=6.0.0"
  },
  "dependencies": {
    "@messageformat/core": "^3.0.0",
    "tslib": "^2.2.0"
  },
  "module": "fesm2015/ngneat-transloco-messageformat.mjs",
  "es2020": "fesm2020/ngneat-transloco-messageformat.mjs",
  "esm2020": "esm2020/ngneat-transloco-messageformat.mjs",
  "fesm2020": "fesm2020/ngneat-transloco-messageformat.mjs",
  "fesm2015": "fesm2015/ngneat-transloco-messageformat.mjs",
  "typings": "ngneat-transloco-messageformat.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./ngneat-transloco-messageformat.d.ts",
      "esm2020": "./esm2020/ngneat-transloco-messageformat.mjs",
      "es2020": "./fesm2020/ngneat-transloco-messageformat.mjs",
      "es2015": "./fesm2015/ngneat-transloco-messageformat.mjs",
      "node": "./fesm2015/ngneat-transloco-messageformat.mjs",
      "default": "./fesm2020/ngneat-transloco-messageformat.mjs"
    }
  },
  "sideEffects": false
}